# InSpec Profile .gitignore

# InSpec generated files
vendor/
*.lock
.inspec/

# Reports and output files
reports/
*.json
*.html
*.xml

# OS generated files
.DS_Store
Thumbs.db

# Editor files
*.swp
*.swo
*~
.vscode/
.idea/

# Temporary files
*.tmp
*.temp

# Logs
*.log

# Environment-specific files (keep templates)
!inputs/*.yml

# Sensitive configuration files
secrets/
.env
.env.local

# InSpec cache
cache/